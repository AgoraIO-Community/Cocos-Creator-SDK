{
  "compilerOptions": {
    "sourceMap": false,
    "outDir": "../../",
    "alwaysStrict": true,
    "declaration": true,
    "removeComments": false,
    "module": "commonjs",
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ],
    "target": "es5",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "../src/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
